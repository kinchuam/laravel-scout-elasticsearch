{
    "name": "kinch/laravel-scout-elasticsearch",
    "description": "Elastic Driver for Laravel Scout",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "kinchuam",
            "email": "kinchuam@outlook.com"
        }
    ],
    "keywords": [
        "laravel",
        "scout",
        "elasticsearch",
        "elastic"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "elasticsearch/elasticsearch": ">=7.2",
        "laravel/scout": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|~9.4.0"
    },
    "autoload": {
        "psr-4": {
            "Kinch\\LaravelScoutElasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "tests/laravel/app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Kinch\\LaravelScoutElasticsearch\\LaravelScoutElasticsearchServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
